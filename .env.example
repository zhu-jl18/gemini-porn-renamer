# GPT-Load 基础配置
GEMINI_BASE_URL=http://localhost:3001/proxy/free
GEMINI_API_KEY=在此填写你的GPT-Load密钥

# 接口类型（选择其一）
# - openai_compat: 使用 OpenAI 兼容接口 (推荐)
#   实际端点: {BASE_URL}/v1beta/openai/chat/completions
# - gemini_native: 使用 Gemini 原生接口
#   实际端点: {BASE_URL}/v1beta/models/{model}:generateContent
LLM_TRANSPORT=openai_compat

# 模型配置
MODEL_FLASH=gemini-2.0-flash-exp
MODEL_PRO=gemini-2.0-flash-thinking-exp

# 并发与超时
MAX_CONCURRENCY=32
REQUEST_TIMEOUT=30
RETRY=3

# 日志目录
LOG_DIR=logs

# 命名风格配置
NAMING_STYLES=chinese_descriptive,scene_role,pornhub_style,concise
NAMING_STYLE_CONFIG=examples/naming_styles.yaml
CANDIDATES_PER_STYLE=1
TOTAL_CANDIDATES=5
